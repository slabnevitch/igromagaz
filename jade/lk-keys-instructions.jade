extends _misc/main


block title
	| Ключи и инструкции

block content
	- var current = 0;
	.wrapper
		ul.breadcrumbs
			li.breadcrumbs__item 
				a(href="#").breadcrumbs__link Главная
			li.breadcrumbs__item 
				span.breadcrumbs__link Ключи и инструкции

	.page.page-lk.page-lk-instructions
		.wrapper-smart
			.wrapper-flex.page-lk__wrap
				include _blocks/lk-sidebar
				
				.page-lk__content
					.lk-min-header(data-da="page-lk__wrap,991,first")
						h1 Ключи и файлы для заказа №813393
	
					.lk-keys-instr
						ul.lk-keys-instr__list.accordion(data-accordion)
							
							li.accordion__item.lk-keys-instr__item
								.accordion__link.lk-keys-instr__list__link Другое
									.lk-keys-instr__toggle-icon
										+icon('arrow-down')
								.accordion__content
									ul.lk-keys-instr__sublist.accordion(data-accordion)
										- for (var i=0; i < 5; i++)
											li.accordion__item.lk-keys-instr__subitem
												.accordion__link.lk-keys-instr__sublink Ключи и файлы
													.lk-keys-instr__toggle-text
														span.toggle-down Подробнее
														span.toggle-up Свернуть
												div(class=i>0 ? "accordion__content hidden" : "accordion__content")
													.lk-keys-instr__row.row-lk-keys-instr
														a(href="#").row-lk-keys-instr__title Cyberpunk 2077 (ключ для ПК)
														.row-lk-keys-instr__descr Неполное количество товара по заказу, ключи будут загружены позднее

													.lk-keys-instr__row.row-lk-keys-instr
														a(href="#").row-lk-keys-instr__title Star Wars Battlefront (ключ для ПК)
														.row-lk-keys-instr__key
															.row-lk-keys-instr__key-label Ключ активации:
															a(href="#").row-lk-keys-instr__key-button Показать ключ

													.lk-keys-instr__row.row-lk-keys-instr
														a(href="#").row-lk-keys-instr__title Star Wars Battlefront (ключ для ПК)
														.row-lk-keys-instr__key
															.row-lk-keys-instr__key-label Ключ активации:
															.row-lk-keys-instr__key-value 6F4A3-NBRWL-DQH3X
													
													.lk-keys-instr__row.row-lk-keys-instr
														a(href="#").row-lk-keys-instr__title Dark Souls 2 Scholar of The First Sin (ключ для ПК)
														.row-lk-keys-instr__descr Неполное количество товара по заказу, ключи будут загружены позднее
